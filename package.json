{
  "name": "growcss",
  "version": "0.0.1",
  "description": "GrowCss is a simple and fully customizable UI framework powerd by stylus, postcss and node.",
  "author": {
    "name": "Daniel Bannert",
    "email": "d.bannert@anolilab.de"
  },
  "homepage": "http://growcss.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/growcss/growcss.git"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "grunt test -v --stack -d",
    "install": "bower install"
  },
  "bugs": {
    "url": "https://github.com/growcss/growcss/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 0.10.1"
  },
  "style": "dist/css/growcss.css",
  "main": "./dist/js/npm",
