    "main": "dist/js/growcss",
    "ignore": ["dist/js/npm"],
    "directories": {
      "lib": "dist"
    },
    "shim": {
      "dist/js/growcss": {
        "deps": ["jquery"],
        "exports": "$"
      }
    },
    "dependencies": {
      "jquery": "*"
    }
  },
  "dependencies": {
    "babel-runtime": "5.8.25"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "bower": "~1.5.3",
    "chai": "3.3.0",
    "cssnano": "^3.0.1",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.1",
    "grunt-bump": "^0.6.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-jade": "^0.15.0",
    "grunt-contrib-jshint": "^0.11.2",
    "grunt-contrib-stylus": "^0.22.0",
    "grunt-contrib-uglify": "~0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-exec": "^0.4.6",
    "grunt-jscs": "^2.1.0",
    "grunt-jsonlint": "^1.0.4",
    "grunt-karma": "^0.12.1",
    "grunt-newer": "^1.1.1",
    "grunt-stylint": "^0.1.2",
    "is-travis": "^1.0.0",
    "jshint-stylish": "^2.0.1",
    "karma": "^0.13.10",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-detect-browsers": "^2.0.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-opera-launcher": "^0.3.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.4",
    "load-grunt-config": "^0.17.2",
    "load-grunt-tasks": "^3.2.0",
    "mocha": "^2.3.3",
    "npm-shrinkwrap": "^200.4.0",
    "phantomjs": "^1.9.17",
    "pixrem": "^3.0.0",
    "postcss": "^5.0.4",
    "postcss-reporter": "^1.2.1",
    "poststylus": "^0.2.1",
    "sinon": "^1.17.1",
    "time-grunt": "^1.2.1",
    "typographic": "^2.9.3"
  },
  "keywords": [
    "css",
    "js",
    "stylus",
    "mobile-first",
    "responsive",
    "framework",
    "web",
    "babel",
    "grunt",
    "bower",
    "front-end",
    "development",
    "build",
    "ivory",
    "stylus",
    "medve",
    "postcss"
